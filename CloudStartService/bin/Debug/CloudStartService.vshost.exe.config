<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="CloudService">
      <section name="GoogleSetting" type="System.Configuration.NameValueSectionHandler"/>
      <!-- <section name="AmazonSetting" type="System.Configuration.NameValueSectionHandler"/> -->
    </sectionGroup>
  </configSections>

  <!-- add config here-->
  <CloudService>    
    <GoogleSetting>
      <add key="Operational" value="Start" />
      <add key="TargetHub" value="" /> <!-- only use for stop operation-->
      <add key="Zone" value="asia-southeast1-a" />
      <add key="Project" value="nodal-strength-195007"/>
      <add key="NumberOfHub" value="2" />
      <add key="Hub1" value="group-1-selenium-hub" />
      <add key="NodeList1" value="group-1-selenium-node-1,group-1-selenium-node-2,group-1-selenium-node-3" />
      <add key="Hub2" value="group-2-selenium-hub" />
      <add key="NodeList2" value="group-2-selenium-node-1,group-2-selenium-node-2,group-2-selenium-node-3" />
    </GoogleSetting>
  </CloudService>
  <!-- end config-->

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
</configuration>